<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<!-- [DB연결 사용법] 8. 만든 DTO와 Mapper정의하기 -->
	<typeAliases>
		<typeAlias type="com.alhl.hz.dto.UserDTO" alias="UserDTO"></typeAlias>
		<typeAlias type="com.alhl.hz.dto.SearchLogDTO" alias="SearchLogDTO"></typeAlias>
		<typeAlias type="com.alhl.hz.dto.NoticeDTO" alias="NoticeDTO"></typeAlias>
		<typeAlias type="com.alhl.hz.dto.ShopDTO" alias="ShopDTO"></typeAlias>
		<typeAlias type="com.alhl.hz.dto.Shop_ProductDTO" alias="Shop_ProductDTO"></typeAlias>
		<typeAlias type="com.alhl.hz.dto.CommentDTO" alias="CommentDTO"></typeAlias>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/UserMapper.xml" />
		<mapper resource="mappers/SearchLogMapper.xml" />
		<mapper resource="mappers/NoticeMapper.xml" />
		<mapper resource="mappers/ShopMapper.xml" />
		<mapper resource="mappers/CommentMapper.xml" />
	</mappers>
</configuration>
